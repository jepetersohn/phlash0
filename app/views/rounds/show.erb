
<h1> Deck name: <%= @deck.title %> </h1>
<div class="flip-container" ontouchstart="this.classList.toggle('hover');">
  <div class="flipper">
    <div class='front'>
      <h1> <%= @card.question %> </h1>
    </div>

    <div class='back'>
      <h1> <%= @card.answer %> </h1>
    </div>
  </div>
</div>



